{
    "manifest_version": 3,
    "name": "Golden Thinker Extractor",
    "description": "An extension to extract fields from web pages",
    "version": "1.0",
    "action": {
      "default_popup": "com_goldenthinkerscrapper_content_html/popup.html",
      "default_icon": {
        "16": "com_goldenthinkerscrapper_content_images/icon16.png",
        "32": "com_goldenthinkerscrapper_content_images/icon32.png",
        "48": "com_goldenthinkerscrapper_content_images/icon48.png",
        "128": "com_goldenthinkerscrapper_content_images/icon128.png"
      }
    },
    "permissions": [
      "activeTab",
      "scripting",
      "tabs",
      "cookies",
      "storage",
      "declarativeContent",
      "webRequest"
    ],
    "host_permissions": [
      "https://www.goldenthinker.com/",
      "https://www.subjectivetechnologies.com/"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["com_goldenthinkerscrapper_bundle/content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [ "com_goldenthinkerscrapper_content_html/popup.html", "com_goldenthinkerscrapper_content_styles/popup.css","com_goldenthinkerscrapper_bundle/content.js" ],
        "matches": [ "https://*/*" ]
      }
    ],
    "background": {
      "service_worker": "com_goldenthinkerscrapper_bundle/background.js"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  